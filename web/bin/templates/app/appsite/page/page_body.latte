			body {
				font-family: "Open Sans", "Palatino Linotype", "Book Antiqua", Palatino, serif;
				font-size: 12px;
				padding: 0px;
			}
			body {
				background: #93cede; /* Old browsers */
				background: -moz-linear-gradient(-45deg, #93cede 0%, #75bdd1 41%, #49a5bf 56%, #ffffff 70%); /* FF3.6+ */
				background: -webkit-gradient(linear, left top, right bottom, color-stop(0%, #93cede), color-stop(41%, #75bdd1), color-stop(56%, #49a5bf), color-stop(70%, #ffffff)); /* Chrome,Safari4+ */
				background: -webkit-linear-gradient(-45deg, #93cede 0%, #75bdd1 41%, #49a5bf 56%, #ffffff 70%); /* Chrome10+,Safari5.1+ */
				background: -o-linear-gradient(-45deg, #93cede 0%, #75bdd1 41%, #49a5bf 56%, #ffffff 70%); /* Opera 11.10+ */
				background: -ms-linear-gradient(-45deg, #93cede 0%, #75bdd1 41%, #49a5bf 56%, #ffffff 70%); /* IE10+ */
				background: linear-gradient(135deg, #93cede 0%, #75bdd1 41%, #49a5bf 56%, #ffffff 70%); /* W3C */
				filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#93cede', endColorstr='#ffffff', GradientType=1); /* IE6-9 fallback on horizontal gradient */

			}
			select {
