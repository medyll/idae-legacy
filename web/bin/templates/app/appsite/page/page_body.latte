		<script>
			show_glue_accept = function () {
				$('glue_zone').hide();
				$('glue_accept').show();
				$('glue_wait').hide();
				hide_glue_zone();
			}
			show_glue_zone = function () {
				$('glue_zone').show();
			}
			hide_glue_zone = function () {
				$('glue_zone').hide();
			}
			subscribe_glue = function () {
				if ($('glue_input').value == '') return;
				$('glue_input').hide();
				$('glue_button').hide();
				$('glue_cancel').show();
				$('glue_wait').show();
				socket_keep_on.emit("subscribe", { APPID: "{session_id}", id: '{session_id}', MSG: $('glue_input').value  });
			}
			unsubscribe_glue = function () {
				socket_keep_on.emit("unsubscribe", { APPID: "{session_id}", id: '{session_id}', MSG: $('glue_input').value  });
			}
			var socket_keep_on = io(':3001/keep_on');
			// signaler connection / request_glue
			socket_keep_on.on('connect', function () {
				socket_keep_on.emit("request_glue", {id: '{session_id}', APPID: '{session_id}'});
			});
			//
			socket_keep_on.on('reconnect', function () {
				socket_keep_on.emit("request_glue", {  id: '{session_id}', APPID: '{session_id}' });
			});
			// glue_reserve
			socket_keep_on.on('glue_reserve', function (data) {
				console.log('glue_reserve ', data);
			});
			// glue_reserve
			socket_keep_on.on('mitete', function (data) {
				show_glue_accept()
				// alert('mitete ');
			});
			//
			socket_keep_on.on('hide_glue', function () {
				hide_glue_zone();
			});

			socket_keep_on.on('show_glue', function () {
				show_glue_zone();
			});

			var timeMouse;
			$('body').on('click', '.menu-icon', function (event) {

			}.bind(this));
			//  timeMouse = setTimeout(function(){ajaxV
			Event.observe('body', 'clisk', function (event) {
				x = Event.pointerX(event);//event.x+document.body.scrollLeft;
				y = Event.pointerY(event);//event.y+document.body.scrollTop;
				clearTimeout(timeMouse);
				//timeMouse = setTimeout(function(){ajaxValidation('test','red')},1250);
			}.bind(this));
			//
			var menuTop ;
			Event.observe(window, 'scroll', function() {
				top = $(document.body).getBoundingClientRect().top
				top_c = $('content').getBoundingClientRect().top
				top_m = $('header_site_menu').getBoundingClientRect().top
				top_h = $('header_site_menu').getBoundingClientRect().height
				// console.log(top_c,top,top_m)
				if(top_c  <= top_h){
					$('header_site_menu').setStyle({ position:'fixed',top:0,width:'100%'});
					$('header_site_menu_more').show();
				}else{
					$('header_site_menu').setStyle({ position:'relative',zIndex:'10000' })
					$('header_site_menu_more').hide();
				}
				if(top_c <= 0){
					// $('scroll_menu').setStyle({ position:'fixed',top:0 });
					// $('overlay_main').setStyle({ position:'fixed','top':0 });
				}else{
					// $('scroll_menu').setStyle({ position:'relative',zIndex:'10000' })  ;
					// $('overlay_main').setStyle({ position:'absolute',top:'' });
				}
			}.bind(this));
			//
			document.observe('dom:loaded', function () {
				menuTop = $('header_site_menu').offsetTop;
				$$('.toggler').each(function (node, index) {
					new autoToggle(node)  ;
				}   );
				$$('.slideBox .slideBoxIn').each(function (node, index) {
					new myddeSlideBox(node)  ;
				}   );
				document.domain = document.domain;
				$$('a.ancre').each(function (node, index) {
					var to = $$('a[name=' + $(node).readAttribute('for') + ']').first()
					$(node).observe('click', function (event) {
						if ($$('[receiver=' + $(node).readAttribute('for') + ']').size() != 0) {
							$$('[receiver=' + $(node).readAttribute('for') + ']').first().toggleContent();
							Event.stop(event)
						} else {
							Event.stop(event)
							//new Effect.ScrollTo('body');
							document.location.hash = $(node).readAttribute('for')
						}

					}.bind(this))
				})

			})
			regAdvert = function (id) {
				ajaxValidation('clickadv', '', 'idadvert=' + id);
			}
		</script>
